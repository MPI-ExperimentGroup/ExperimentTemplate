<xs:simpleType name="stimulusIdentifier">
<xs:restriction base="xs:string">
<xs:pattern value="[^-]{1,}"/>
</xs:restriction>
</xs:simpleType>
<xs:simpleType name="stimulusTag">
<xs:restriction base="xs:string">
<xs:pattern value="([^\p{P}\W]*[_]*){1,}"/>
</xs:restriction>
</xs:simpleType>
<xs:simpleType name="stimulusTags">
<xs:restriction base="xs:string">
<xs:pattern value="([^\p{P}\W]*[ _]*)*"/>
