</xs:restriction>
</xs:simpleType>
<xs:simpleType name="groupPhases">
<xs:restriction base="xs:string">
<xs:pattern value="(-|([A-Za-z_0-9]+(,[A-Za-z_0-9]+)*))(:(-|([A-Za-z_0-9]+(,[A-Za-z_0-9]+)*)))*"/>
