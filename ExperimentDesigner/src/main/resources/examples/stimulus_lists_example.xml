        <stimulus identifier="d1e284" label="d1e284" tags="all_stimuli" />
        <stimulus identifier="d1e290" label="d1e290" tags="all_stimuli" />
        <stimulus identifier="d1e228" label="d1e228" tags="all_stimuli" />
